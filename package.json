    "async": "^3.2.4",
    "autoprefixer": "^10.4.13",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "floating-vue": "^5.2.2",
    "lodash": "^4.17.21",
    "pinia": "^2.0.28",
    "postcss": "^8.4.19",
    "query-string": "^8.1.0",
    "radix-vue": "^1.9.12",
    "sass": "^1.56.2",
    "sweetalert2": "^11.7.12",
